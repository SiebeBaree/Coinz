{
    "name": "support",
    "version": "2.0.0",
    "description": "The official support bot for the Coinz bot.",
    "main": "dist/index.js",
    "scripts": {
        "start": "pnpm run build && node --env-file .env ./dist/index.js",
        "dev": "concurrently \"pnpm run build -- --watch\" \"nodemon --env-file .env ./dist/index.js\"",
        "build": "tsc",
        "lint": "prettier --check . && eslint --ext .ts --format stylish src",
        "format": "prettier --write . && eslint --ext .ts --fix --format stylish src",
        "register-commands": "pnpm run build && node --env-file .env ./dist/register.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SiebeBaree/Coinz.git"
    },
    "author": "SiebeBaree",
    "license": "SEE LICENSE IN LICENSE",
    "bugs": {
        "url": "https://github.com/SiebeBaree/Coinz/issues"
    },
    "homepage": "https://github.com/SiebeBaree/Coinz#readme",
    "keywords": [
        "economy",
        "discord",
        "bot",
        "typescript",
        "discord.js",
        "coinz",
        "support",
        "helper"
    ],
    "dependencies": {
        "bufferutil": "^4.0.8",
        "discord.js": "^14.14.1",
        "mongoose": "^8.0.3",
        "utf-8-validate": "^6.0.3",
        "winston": "^3.11.0",
        "zlib-sync": "^0.1.9"
    },
    "devDependencies": {
        "@types/node": "^20.10.5",
        "eslint": "^8.56.0",
        "eslint-config-neon": "^0.1.57",
        "eslint-formatter-pretty": "^6.0.0",
        "prettier": "^3.1.1",
        "typescript": "^5.3.3"
    }
}
